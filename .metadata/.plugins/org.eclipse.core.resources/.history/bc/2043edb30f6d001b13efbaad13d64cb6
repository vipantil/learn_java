
public class AssignmentOperators {

	public static void main(String[] args) {
		// assignment operators are 
		// =    +=     -+    *=      /=       %=
		// for example X += y meaning of this is x = x+y
		int n=5; // int5
		n *= n += 10; // n = n*(n+10) 5*5+10 = 75
		System.out.println(n); // ans 75
		
		//what is meaning of associativity 
		//why it is 75 = n is equal to 5 ( int5 ) 
		
		int a=3;
		a *= a =+ 5;
		

	}

}
